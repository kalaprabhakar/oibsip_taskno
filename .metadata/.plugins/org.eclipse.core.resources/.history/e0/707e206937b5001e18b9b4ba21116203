import com.kuu.dao.IReservationDAO;

@Service("servie")
public class ReservationServiceImpl implements IReservationService {
	
	@Autowired
	private IReservationDAO dao ;

	@Override
	public boolean checkLoginDetails() throws Exception {
		
		List<String> list = new ArrayList<>();
		
		list = dao.userLogin();
		
		return false;
	}

}
